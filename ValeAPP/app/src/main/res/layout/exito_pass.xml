        android:fontFamily="@font/roboto_medium"
        android:gravity="center"
        android:text="@string/bien_hecho_entrando_a_tu_cuenta"
        android:textColor="#000"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
