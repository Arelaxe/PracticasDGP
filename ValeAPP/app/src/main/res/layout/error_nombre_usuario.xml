<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/errorNombreUsuarioAtras"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ErrorNombreUsuario">

    <TextView
        android:id="@+id/errorNombreUsuario"
        android:layout_width="375dp"
        android:layout_height="301dp"
        android:contentDescription="@string/error_en_el_nombre_de_usuario"
        android:fontFamily="@font/roboto_medium"
        android:gravity="center"
        android:text="@string/error_en_el_nombre_de_usuario"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.144" />

    <ImageButton
        android:id="@+id/idBotonVolverNombreUsuario"
        android:layout_width="150dp"
        android:layout_height="133dp"
        android:layout_marginTop="152dp"
        android:background="#000000"
        android:contentDescription="@string/bot_n_que_permite_volver_a_la_pantalla_para_introducir_el_nombre_de_usuario"
        android:scaleType="fitCenter"
        android:scaleX="1"
        android:scaleY="1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/errorNombreUsuario"
        app:srcCompat="@drawable/abc_vector_test" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/vuelve_a_intentarlo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idBotonVolverNombreUsuario" />

</androidx.constraintlayout.widget.ConstraintLayout>